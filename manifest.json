{
  "name": "countdown",
  "version": "1.0",
  "description": "display a certain countdown!",
  "permissions": ["storage", "declarativeContent", "activeTab"],
  "chrome_url_overrides": {
    "newtab":"newpage.html"
  },
  "browser_action": {
    "default_icon": "hourglass.png"
  },
  "permissions":["tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
  "manifest_version": 2
}